    "from typing import List \n",
    "def find_even(input_lst:List)->List:\n",
    "    out_lst= [element for element in input_lst if element%2==0]\n",
    "    return out_lst\n",
    "\n",
    "inp=[2,3,4,5,6,7,7,8,10,12,14]\n",
    "print(find_even(inp))"
   ]
